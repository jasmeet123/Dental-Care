<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_past_request"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.dentalcareapp.main.PastRequestActivity">


    <ListView
        android:layout_width="match_parent"
        android:id="@+id/past_request_list"
        android:layout_height="match_parent"
        android:layout_alignParentTop="true"
        android:visibility="gone"
        android:layout_alignParentLeft="true"

        android:layout_alignParentStart="true" />

    <TextView
        android:text="@string/no_request"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:layout_editor_absoluteX="141dp"
        android:layout_gravity="center"
        tools:layout_editor_absoluteY="30dp"
        android:id="@+id/no_history_request"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"
        android:layout_margin="10dp"

        android:visibility="visible"
        android:textAppearance="@style/ProfileTitle" />
</FrameLayout>
